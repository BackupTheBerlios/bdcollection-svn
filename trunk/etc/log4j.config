# using also a file
# log4j.rootLogger=debug, stdout, R
log4j.rootLogger=debug, stdout
	
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout

# Pattern to output the caller's file name and line number.
log4j.appender.stdout.layout.ConversionPattern=%5p [%t] (%c:%L) - %m%n

# Print only messages of level INFO or above in the package
log4j.logger.collector.data=error
#log4j.logger.collector.data.Enregistrement=debug
#log4j.logger.collector.data.XML=error
#log4j.logger.collector.data.SearchOperator=debug
#log4j.logger.collector.data.SearchedElement=debug
log4j.logger.collector.gui=warn
log4j.logger.collector.gui.JBaseDeDonnees=debug
log4j.logger.collector.control=warn
log4j.logger.collector.control.Actions=debug
log4j.logger.collector.control.DialogPrint=debug

